public class Motorcycle extends MotorVehicle implements Engine {

	public void start () {
		
		running = true;
		System.out.println("Status: " + running + "\nColor: " + color + "\nMotorcycle" + "\nStarted" ) ;
	}
	
	public void stop () {
		
		running = false;
		System.out.println("Status: " + running + "\nColor: " + color + "\nMotorcycle" + "\nStopped" ) ;
	}
	
	
}
